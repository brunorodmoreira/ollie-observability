    "lint": "eslint \"src/**/*.ts*\"",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.7",
    "@vtex/api": "^6.46.0",
    "esbuild": "^0.19.5",
    "@types/node": "^12.0.0",
    "esbuild-node-externals": "^1.9.0",
    "eslint": "^8.52.0",
    "eslint-config-custom": "*",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsconfig": "*",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@vtex/api": "^6.46.0"
  },
  "publishConfig": {
    "access": "public"
